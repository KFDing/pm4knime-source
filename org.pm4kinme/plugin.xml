<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

    <extension point="org.knime.workbench.repository.categories">
        <category icon="icons/pm4knime.png" level-id="processmining" name="Process Mining" path="/"/>
        <category icon="icons/category_reader.png" level-id="io" name="IO" path="/processmining"/>
        <category level-id="discovery" name="Discovery" path="/processmining"/>
   </extension>
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/processmining/io" factory-class="org.pm4kinme.node.io.read.XLogReaderNodeFactory" id="org.pm4kinme.node.io.read.XLogReaderNodeFactory"/>
       <node category-path="/processmining/io" factory-class="org.pm4knime.ding.io.petrinet.reader.PetrinetReaderNodeFactory" id="org.pm4knime.ding.io.PetrinetReaderNodeFactory"/>  
       <node category-path="/processmining/discovery" factory-class="org.pm4kinme.node.discovery.alpha.AlphaMinerNodeFactory" id="org.pm4kinme.node.discovery.alpha.AlphaMinerNodeFactory"/>
      <node category-path="/processmining/discovery" factory-class="org.pm4knime.ding.inductiveminer.InductiveMinerNodeFactory" id="org.pm4knime.ding.inductiveminer.InductiveMinerNodeFactory"/>
</extension>
    <extension point="org.knime.core.PortType">
       <portType hidden="false" name="Event Log" objectClass="org.pm4kinme.portobject.XLogPortObject" objectSerializer="org.pm4kinme.portobject.XLogPortObject$XLogPortObjectSerializer" specClass="org.pm4kinme.portobject.XLogPortObjectSpec" specSerializer="org.pm4kinme.portobject.XLogPortObjectSpec$XLogPortObjectSpecSerializer">
       </portType>
       <portType hidden="false" name="Petri Net" objectClass="org.pm4kinme.portobject.PetriNetPortObject" objectSerializer="org.pm4kinme.portobject.PetriNetPortObject$PetriNetPortObjectSerializer" specClass="org.pm4kinme.portobject.PetriNetPortObjectSpec" specSerializer="org.pm4kinme.portobject.PetriNetPortObjectSpec$PetriNetPortObjectSpecSerializer">
       </portType>
    </extension>
</plugin>
